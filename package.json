      "default": "./.medusa/server/src/admin/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anubus298/medusa-backup.git"
  },
  "bugs": {
    "url": "https://github.com/anubus298/medusa-backup/issues"
  },
  "homepage": "https://github.com/anubus298/medusa-backup#readme",
  "keywords": [
    "medusa-plugin",
    "medusa-backup",
    "medusa",
    "medusajs",
    "medusa.js",
    "medusa-v2",
    "medusa-database-backup",
    "postgresql-backup",
    "medusa-restore",
    "medusa-s3-backup",
    "medusa-admin-plugin",
    "medusa-plugin-other"
  ],
  "scripts": {
    "build": "medusa plugin:build",
    "dev": "medusa plugin:develop",
    "prepublishOnly": "medusa plugin:build"
  },
  "devDependencies": {
    "@medusajs/admin-sdk": "2.10.1",
